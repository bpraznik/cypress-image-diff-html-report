{
  "name": "cypress-image-diff-html-report",
  "version": "1.0.0",
  "description": "A HTML reporting module for Cypress-Image-Diff to parse the JSON output to a beautiful report",
  "main": "./lib/dist",
  "scripts": {
    "prepare": "husky install",
    "install:server": "cd lib/server && npm i",
    "install:ui": "cd lib/ui && npm i",
    "postinstall": "run-p install:*",
    "start": "nodemon lib/server/src/index.js",
    "format": "prettier . --write --list-different",
    "lint": "eslint . --fix --max-warnings=0",
    "test": ""
  },
  "bin": "./bin/index.js",
  "author": {
    "name": "Kien Ha Trung",
    "email": "kienht.dev@gmail.com"
  },
  "keywords": [
    "cypress",
    "visual",
    "testing",
    "regression",
    "comparison",
    "html report"
  ],
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "lodash": "^4.17.21"
  }
}
